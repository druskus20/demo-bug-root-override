<!doctype html>
<html data-theme="dark" lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title @text="title || meta.title"></title>
  <meta name="description" :content="meta.description">

  <!-- Favicon -->
  <link rel="icon" type="image/x-icon" :href=url("/favicon/favicon.ico")>
  <link rel=" icon" type="image/png" sizes="32x32" :href=url("/favicon/favicon-32x32.png")>
  <link rel="icon" type="image/png" sizes="16x16" :href=url("/favicon/favicon-16x16.png")>
  <link rel="apple-touch-icon" sizes="180x180" :href=url("/favicon/apple-touch-icon.png")>

  <!-- Global Styles  -->
  <link webc:keep rel="stylesheet" :href=url("/css/styles.css")>

  <!-- Styles and JS generated by WebC -->
  <link webc:keep rel="stylesheet" :href=getBundleFileUrl('css')>
  <script webc:keep defer :src=getBundleFileUrl('js')></script>
</head>

<body>
  <header>
  </header>
  <main>
    <div @html="content"></div>
  </main>
  <footer>
  </footer>
</body>

</html>
